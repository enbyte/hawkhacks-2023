{
    "manifest_version": 3,
    "name": "WebContext",
    "description": "WebContext: add context to claims on pages you view",
    "version": "0.1",

    

    "action":{
        "default_popup": "index.html"
    },

    "background": {
        "service_worker": "background.js"
    },


    "permissions":
     ["contextMenus",
       "tabs",
       "activeTab",
       "storage"]



}